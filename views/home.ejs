<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= startingContent %></p>

<% for( let i = 0; i < newPost.length; i++ ) { %>
<h1><%= newPost [i].title %></h1>
<p>
  <%= newPost [i].content.substring(0, 150) + '...' %>
  <a href="/posts/<%= (newPost [i].title) %>">Read More</a>
</p>
<% } %> <%- include('partials/footer') -%>
